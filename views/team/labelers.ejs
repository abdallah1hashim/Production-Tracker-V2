    <body>
        <header>
            <%- include("../includes/navigation.ejs") %>
        </header>
        <main>
            <div class="container">

            
            <div class="row">
              <%- include("../includes/side-nav.ejs") %>
                <div class=" main col-9 ">
                    <div class="info">
                        <div class="h2-con w-100">
                          <h2>My Team</h2>
                        </div>
                        <div class="scroller">
                        <table>
                            <tr>
                                <th>C</th>
                                <th>LabelerID</th>
                                <th>Name</th>
                                <th>#</th>
                                <th>#</th>

                            </tr>
                            <% labelers.forEach(el => {%>
                            <tr>
                                <td>C-<%= el.device %></td>
                                <td><%= el.username %></td>
                                <td><%= el.name %></td>
                                <td><a class="edit" href="#">edit</a></td>
                                <td><a class="delete" href="#">delete</a></td>
                            </tr>
                            <% }) %>
                        </table>
                        <button  class="add"><a href="/create-labeler">+</a></button>
                            </div>
                      </div>
                </div>
            </div>
        </div>
        </main>
    </body>
