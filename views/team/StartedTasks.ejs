<%- include("../includes/head.ejs") %>
<link rel="stylesheet" href="/css/labeler.css">
<link rel="stylesheet" href="/css/table.css">
</head>
    <body>
        <header>
            <%- include("../includes/navigation.ejs") %>
        </header>
        <main>
            <div class="container">

            
            <div class="row">
              <%- include("../includes/side-nav.ejs") %>
                <div class=" main col-9 ">
                    <div class="info">
                        <div class="h2-con w-100">
                          <h2>Started Tasks</h2>
                        </div>
                        <div class="scroller">

                            <table>
                                <tr>
                                    <th>C</th>
                                    <th>Task ID</th>
                                    <th>Queue</th>
                                    <th>started</th>
                                    <th>Obj</th>
                                    <th>#</th>
                                    <th>#</th>
                                </tr>
                                <% tasks.forEach(el => {%>
                                    <tr>
                                        <td>C-<%= el.labelerId.device %></td>
                                        <td><%= el.id %></td>
                                        <td><%= el.queueName?.name?.toUpperCase() ?? "no queue" %></td>
                                        <td><%= el.startDate.split(",")[0] %></td>
                                        <td><%= el.StartednumObj %></td>
                                        <td><a class="edit" href="/edit-task/<%= el._id %>">Edit</a></td>
                                        <td><form  method="post" action="/delete-task"><input type="hidden" name="taskId" value="<%= el._id %>"><button class="delete">Delete</button></form></td>
                                    </tr>
                                    <% }) %>
                                </table>
                                <button class="add">+</button>
                            </div>
                      </div>
                </div>
            </div>
        </div>
        </main>
    </body>
<%- include("../includes/end.ejs") %>