<%- include("../includes/head.ejs") %>
<link rel="stylesheet" href="css/labeler.css">
<link rel="stylesheet" href="css/table.css">
<script defer src="scripts/queue.js"></script>
</head>
    <body>
        <header>
            <%- include("../includes/navigation.ejs") %>
        </header>
        <main>
            <div class="container">

            
            <div class="row">
              <%- include("../includes/side-nav.ejs") %>
                <div class=" main col-9 ">
                    <div class="info">
                        <div class="h2-con w-100">
                          <h2>My Team</h2>
                        </div>
                        <table>
                            <tr>
                                <th>Queue name</th>
                                <th>#</th>
                                <th>#</th>

                            </tr>
                            <% q.forEach(el => {%>
                            <tr>
                                    <td class="name"><%= el.name.toUpperCase() %></td>
                                    <td class="edit" data-edit="<%= el._id %>">edit</td>
                                <td><form action="delet-queue/<%= el._id %>" method="post"><button class="delete" type="submit">Delete</button></form></td>
                            </tr>
                            <% }) %>
                        </table>
                      </div>
                </div>
            </div>
        </div>
        </main>
    </body>
<%- include("../includes/end.ejs") %>