        <header>
            <%- include("../includes/navigation.ejs") %>
        </header>
        <main>
            <div class="container">

            
            <div class="row">
              <%- include("../includes/side-nav.ejs") %>
                <div class=" main col-9 ">
                    <div class="info">
                        <div class="h2-con w-100">
                          <h2>Start Task</h2>
                        </div>
                        <div class="details">
                         <form action="/start-task" method="post">
                            <p>number of objects: </p>
                            <input type="text" name="numObj" required>
                            <p>Queue Name:</p>
                            <input type="text" name="queueName" />
                            <p>Task ID:</p>
                            <input type="text" name="TaskId" value>
                            <input type="hidden" name="labelerId" value="<%= labelerDetails._id %>">
                            <button type="submit">Start</button>
                         </form>
                        </div>
                      </div>
                </div>
            </div>
        </div>
