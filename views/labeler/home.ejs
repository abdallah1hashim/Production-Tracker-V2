<%- include("../includes/head.ejs") %>
<link rel="stylesheet" href="/css/labeler.css">
</head>
    <body>
        <header>
            <%- include("../includes/navigation.ejs") %>
        </header>
        <main>
            <div class="container">

            
            <div class="row">
              <%- include("../includes/side-nav.ejs") %>
                <div class=" main col-9 ">
                    <div class="info">
                      <% if(success && success.length > 0){ %> <div class="success"><p><%= success %></p></div><% } %>
                      <% if(error && error.length > 0){ %> <div class="success"><p><%= error %></p></div><% } %>
                        <div class="h2-con w-100">
                          <h2>User Details</h2>
                        </div>
                        <div class="details">
                          <div class="user-icon">
                            <i class="fa-solid fa-user"></i>
                          </div>
                          <div class="personal-info">
                            <h3><%= labelerDetails.name %></h3>
                            <div class="personal-info-text">
                              <p>EMAIL: <%= labelerDetails.username%>@meti.ai</p>
                              <p>SHIFT: <%= labelerDetails.seniorId.shiftName %></p>
                              <p>LOCATION: <%= labelerDetails.location.locationName %></p>
                              <p>TEAM: <%= labelerDetails.team.name %></p>
                              <p>DEVICE: C-<%= labelerDetails.device %></p>
                            </div>
                          </div>
                        </div>
                      </div>
                </div>
            </div>
        </div>
        </main>
    </body>
<%- include("../includes/end.ejs") %>